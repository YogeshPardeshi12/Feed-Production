@model Feed_Production.Models.MMenu_Models
@*@{
        ViewBag.Title = "MMenuView";
    }*@

<h2 class="text-center bg-success">MMenuView</h2>
@using (Html.BeginForm("Post", "MMenu", FormMethod.Post))
{
    <div class="row">
        <div class="col-md-3">
            <div class="form-group">
                <label>MenuId</label>
                @Html.TextBoxFor(m => m.MenuId, new { @class = "form-control" })
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group">
                <label>Module</label>
                @Html.TextBoxFor(m => m.Module, new { @class = "form-control" })
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group">
                <label>MenuName</label>
                @Html.TextBoxFor(m => m.MenuName, new { @class = "form-control" })
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group">
                <label>ParentId</label>
                @Html.TextBoxFor(m => m.ParentId, new { @class = "form-control" })
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group">
                <label>Tag</label>
                @Html.TextBoxFor(m => m.Tag, new { @class = "form-control" })
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group">
                <label>EntryType</label>
                @Html.TextBoxFor(m => m.EntryType, new { @class = "form-control" })
            </div>
        </div>
    </div>
    <button type="submit" class="btn-danger">Save</button>
    <button type="reset" class="btn-danger">Reset</button>
}
@Scripts.Render("~/bundles/jqueryval")


