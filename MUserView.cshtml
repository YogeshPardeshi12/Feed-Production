@model Feed_Production.Models.MUser_Models
@*@{
        ViewBag.Title = "MUserView";
    }*@

<h2 class="text-center bg-primary">MUserView</h2>
@using (Html.BeginForm("Post", "MUser", FormMethod.Post))
{
    <div class="row">
        <div class="col-md-3">
            <div class="form-group">
                <label>Module</label>
                @Html.TextBoxFor(m => m.Module, new { @class = "form-control" })
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group">
                <label>UserId</label>
                @Html.TextBoxFor(m => m.UserId, new { @class = "form-control" })
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group">
                <label>UserName</label>
                @Html.TextBoxFor(m => m.UserName, new { @class = "form-control" })
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group">
                <label>UserPwd</label>
                @Html.TextBoxFor(m => m.UserPwd, new { @class = "form-control" })
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group">
                <label>UserType</label>
                @Html.TextBoxFor(m => m.UserType, new { @class = "form-control" })
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group">
                <label>EntryType</label>
                @Html.TextBoxFor(m => m.EntryType, new { @class = "form-control" })
            </div>
        </div>
    </div>
    <button type="submit" class="btn-danger">Save</button>
    <button type="reset" class="btn-danger">Reset</button>
}
@Scripts.Render("~/bundles/jqueryval")

